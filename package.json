{
  "name": "twentynineteen",
  "version": "1.0.0",
  "description": "Default WP Theme",
  "bugs": {
    "url": "https://github.com/WordPress/twentynineteen/issues"
  },
  "engines": {
    "node": "8.11.4"
  },
  "scripts": {
    "start:dev": "SHELL=/bin/bash run-p --print-label js:build browser-sync watch",
    "build:style": "node-sass -w styles/sass/ -o styles/css/",
    "build": "run-p \"build:*\"",
    "browser-sync": "browser-sync start --config './config/bs-config.js'",
    "watch": "chokidar \"**/*.scss\" -c \"npm run build\" --initial",
    "devserver": "clear; NODE_ENV=development nodemon --delay 2.5 server",
    "test": "run-p --race --print-label start:dev cy:run",
    "lint": "npx tslint --project ./tsconfig.json",
    "postlint": "npm run tsc",
    "tsc": "npx tsc --pretty",
    "precy:run": "npm run tsc",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "unitTests": "jest",
    "js:build": "clear; webpack ---config webpack.config.js",
    "_build:images": "imageoptim --jpegmini './img/gallery/**/*.jpg' './img/gallery/**/*.jpeg'; cd img; imageoptim --imagealpha '**/*.png'",
    "_build:renamefiles": "renamer --find jpeg --replace jpg './img/gallery/**';  renamer --find '/-/g' --replace ' '  './img/gallery/**'; renamer --find '/s/g'  './img/gallery/**';  renamer -p case --case lower './img/gallery/**' --force"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "browser-sync": "^2.23.5",
    "express": "^4.15.2",
    "fancybox": "^3.0.1",
    "http-server": "^0.11.1",
    "jpegmini": "^0.1.4",
    "jquery": "^3.3.1",
    "jquery.scrollto": "^2.1.2",
    "mandrill-api": "^1.0.45",
    "modernizr": "^3.7.1",
    "pug": "^2.0.0-beta11",
    "renamer": "^1.1.4",
    "run-p": "0.0.0",
    "rxjs": "^5.4.1",
    "slick-carousel": "^1.8.1",
    "tooltipster": "^4.2.6",
    "ts-loader": "^5.3.3"
  },
  "devDependencies": {
    "@types/jquery.scrollto": "^1.4.28",
    "@types/node": "^10.12.18",
    "@wordpress/browserslist-config": "^2.2.2",
    "autoprefixer": "^9.1.5",
    "chokidar-cli": "^3.0.0",
    "cypress": "^3.1.4",
    "expose-loader": "^0.7.5",
    "jest": "^23.6.0",
    "knex": "^0.12.9",
    "node-sass": "^4.13.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.1",
    "postcss-focus-within": "^3.0.0",
    "rtlcss": "^2.4.0",
    "script-loader": "^0.7.2",
    "tsc": "^1.20150623.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  }
}
